<androidx.core.widget.NestedScrollView
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="?attr/colorSurface"
	android:padding="16dp">
	<LinearLayout
		android:id="@+id/linear4"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="vertical">
		<com.google.android.material.textview.MaterialTextView
			android:id="@+id/textview9"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginBottom="24dp"
			android:text="New Project"
			android:textSize="20sp"
			android:textStyle="bold"
			android:layout_gravity="start"
			android:textColor="?attr/colorOnSurface" />
		<FrameLayout
			android:id="@+id/view5"
			android:layout_width="100dp"
			android:layout_height="100dp"
			android:layout_marginBottom="24dp"
			android:layout_gravity="center_horizontal">
			<ImageView
				android:id="@+id/scIcon"
				android:layout_width="50dp"
				android:layout_height="50dp"
				android:src="@drawable/android_app_icon_sdk_21"
				android:scaleType="fitCenter"
				android:layout_gravity="center_horizontal|center_vertical" />
		</FrameLayout>
		<com.google.android.material.textview.MaterialTextView
			android:id="@+id/textview10"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginBottom="8dp"
			android:text="Tap to create new Icon"
			android:textSize="14sp"
			android:layout_gravity="center_horizontal"
			android:textColor="?attr/colorOnSurface" />
		<com.google.android.material.textview.MaterialTextView
			android:id="@+id/textview17"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginBottom="4dp"
			android:text="Enter application name"
			android:textSize="12sp"
			android:textColor="?attr/colorOnSurface"
			android:layout_marginStart="16dp" />
		<com.google.android.material.textfield.TextInputLayout
			android:id="@+id/appNameInputLayout"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginBottom="16dp"
			android:layout_marginStart="32dp"
			android:layout_marginEnd="16dp"
			style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
			app:hintTextColor="?attr/colorOnSurface">
			<com.google.android.material.textfield.TextInputEditText
				android:id="@+id/srcName"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:text="New Project"
				android:textSize="12sp"
				android:hint="Application name"
				android:textColor="?attr/colorOnSurface"
				android:textColorHint="?attr/colorOnSurfaceVariant" />
		</com.google.android.material.textfield.TextInputLayout>
		<com.google.android.material.textview.MaterialTextView
			android:id="@+id/textview12"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginBottom="4dp"
			android:text="Package name"
			android:textSize="12sp"
			android:textColor="?attr/colorOnSurface"
			android:layout_marginStart="16dp" />
		<com.google.android.material.textfield.TextInputLayout
			android:id="@+id/pkgNameInputLayout"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginBottom="16dp"
			android:layout_marginStart="32dp"
			android:layout_marginEnd="16dp"
			style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
			app:hintTextColor="?attr/colorOnSurface">
			<com.google.android.material.textfield.TextInputEditText
				android:id="@+id/prgPkgName"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:text="com.my.newproject"
				android:textSize="12sp"
				android:hint="Package name"
				android:textColor="?attr/colorOnSurface"
				android:textColorHint="?attr/colorOnSurfaceVariant" />
		</com.google.android.material.textfield.TextInputLayout>
		<com.google.android.material.textview.MaterialTextView
			android:id="@+id/textview13"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginBottom="4dp"
			android:text="Project name"
			android:textSize="12sp"
			android:textColor="?attr/colorOnSurface"
			android:layout_marginStart="16dp" />
		<com.google.android.material.textfield.TextInputLayout
			android:id="@+id/projectNameInputLayout"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginBottom="24dp"
			android:layout_marginStart="32dp"
			android:layout_marginEnd="16dp"
			style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
			app:hintTextColor="?attr/colorOnSurface">
			<com.google.android.material.textfield.TextInputEditText
				android:id="@+id/projectName"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:text="NewProject"
				android:textSize="12sp"
				android:hint="Project name"
				android:textColor="?attr/colorOnSurface"
				android:textColorHint="?attr/colorOnSurfaceVariant" />
		</com.google.android.material.textfield.TextInputLayout>
		<HorizontalScrollView
			android:id="@+id/hscroll1"
			android:layout_width="match_parent"
			android:layout_height="wrap_content">
			<LinearLayout
				android:id="@+id/linear11"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/linear7"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/linear8"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical">
						<FrameLayout
							android:id="@+id/colorAccent"
							android:layout_width="48dp"
							android:layout_height="48dp"
							android:background="?attr/colorAccent" />
						<com.google.android.material.textview.MaterialTextView
							android:id="@+id/textview18"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="colorAccent"
							android:textSize="12sp"
							android:textColor="?attr/colorOnSurface" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear12"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical">
						<FrameLayout
							android:id="@+id/cP"
							android:layout_width="48dp"
							android:layout_height="48dp"
							android:background="?attr/colorPrimary" />
						<com.google.android.material.textview.MaterialTextView
							android:id="@+id/textview19"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="colorPrimary"
							android:textSize="12sp"
							android:textColor="?attr/colorOnSurface" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear14"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical">
						<FrameLayout
							android:id="@+id/cAD"
							android:layout_width="48dp"
							android:layout_height="48dp"
							android:background="?attr/colorSecondary" />
						<com.google.android.material.textview.MaterialTextView
							android:id="@+id/textview20"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="colorAccentDark"
							android:textSize="12sp"
							android:textColor="?attr/colorOnSurface" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear18"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical">
						<FrameLayout
							android:id="@+id/cPH"
							android:layout_width="48dp"
							android:layout_height="48dp"
							android:background="?attr/colorPrimaryVariant" />
						<com.google.android.material.textview.MaterialTextView
							android:id="@+id/textview22"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="colorPrimaryHighlight"
							android:textSize="12sp"
							android:textColor="?attr/colorOnSurface" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear16"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical">
						<FrameLayout
							android:id="@+id/cPN"
							android:layout_width="48dp"
							android:layout_height="48dp"
							android:background="?attr/colorPrimary" />
						<com.google.android.material.textview.MaterialTextView
							android:id="@+id/textview21"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="colorPrimaryNormal"
							android:textSize="12sp"
							android:textColor="?attr/colorOnSurface" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</HorizontalScrollView>
		<View
			android:id="@+id/linear20"
			android:layout_width="match_parent"
			android:layout_height="18dp" />
		<LinearLayout
			android:id="@+id/linear21"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="horizontal">
			<com.google.android.material.textfield.TextInputLayout
				android:id="@+id/textinputlayout1"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_margin="5dp"
				android:layout_weight="1"
				style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
				app:hintTextColor="?attr/colorOnSurface">
				<com.google.android.material.textfield.TextInputEditText
					android:id="@+id/versionCode"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:text="1"
					android:textSize="12sp"
					android:hint="1 - (Version code)"
					android:textColor="?attr/colorOnSurface"
					android:textColorHint="?attr/colorOnSurfaceVariant"
					android:inputType="number" />
			</com.google.android.material.textfield.TextInputLayout>
			<com.google.android.material.textfield.TextInputLayout
				android:id="@+id/textinputlayout3"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_margin="5dp"
				android:layout_weight="1"
				style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
				app:hintTextColor="?attr/colorOnSurface">
				<com.google.android.material.textfield.TextInputEditText
					android:id="@+id/versionName"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:text="1.0"
					android:textSize="12sp"
					android:hint="1.0 - (Version name)"
					android:textColor="?attr/colorOnSurface"
					android:textColorHint="?attr/colorOnSurfaceVariant" />
			</com.google.android.material.textfield.TextInputLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear6"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal"
			android:gravity="end">
			<com.google.android.material.button.MaterialButton
				android:id="@+id/cancelProject"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:text="Cancel"
				android:textSize="12sp"
				android:textColor="#000000"
				style="@style/Widget.MaterialComponents.Button.TextButton"
				app:strokeColor="?attr/colorOnSurface"
				app:strokeWidth="1dp" />
			<com.google.android.material.button.MaterialButton
				android:id="@+id/createProject"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:text="New Project"
				android:textSize="12sp"
				android:textColor="#000000"
				android:layout_marginStart="16dp"
				style="@style/Widget.MaterialComponents.Button"
				app:backgroundTint="?attr/colorPrimary" />
		</LinearLayout>
	</LinearLayout>
</androidx.core.widget.NestedScrollView>
