<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<TextView
		android:id="@+id/textview3"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="10dp"
		android:layout_marginTop="10dp"
		android:layout_marginRight="10dp"
		android:padding="8dp"
		android:textSize="14sp"
		android:textStyle="bold"
		android:textColor="#424242"
		android:focusable="false"
		android:tag="tv_variant" />
	<LinearLayout
		android:id="@+id/linear12"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="10dp"
		android:layout_marginBottom="5dp"
		android:gravity="center_vertical"
		android:orientation="vertical"
		android:animateLayoutChanges="true"
		android:tag="log_background">
		<LinearLayout
			android:id="@+id/linear13"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/view_leftline"
				android:layout_width="6dp"
				android:layout_height="match_parent"
				android:layout_marginTop="10dp"
				android:layout_marginBottom="10dp"
				android:background="#008DCD"
				android:orientation="horizontal"
				android:layout_marginEnd="10dp" />
			<androidx.cardview.widget.CardView
				android:id="@+id/cardview3"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="10dp"
				android:layout_marginBottom="10dp"
				app:cardCornerRadius="100dp">
				<ImageView
					android:id="@+id/img_user_icon"
					android:layout_width="45dp"
					android:layout_height="45dp"
					android:src="@drawable/app_icon"
					android:scaleType="centerCrop"
					android:focusable="false" />
			</androidx.cardview.widget.CardView>
			<LinearLayout
				android:id="@+id/linear14"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<TextView
					android:id="@+id/tv_title"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="8dp"
					android:paddingRight="8dp"
					android:paddingBottom="2dp"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="#000000"
					android:singleLine="true"
					android:ellipsize="end"
					android:fadingEdgeLength="20dp"
					android:focusable="true"
					android:focusableInTouchMode="true"
					android:marqueeRepeatLimit="marquee_forever"
					android:requiresFadingEdge="horizontal"
					android:scrollHorizontally="true" />
				<TextView
					android:id="@+id/tv_release_note"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="2dp"
					android:paddingRight="8dp"
					android:paddingBottom="8dp"
					android:textSize="12sp"
					android:textColor="#212121"
					android:focusable="false" />
			</LinearLayout>
			<ImageButton
				android:id="@+id/imageview3"
				android:focusable="false"
				android:layout_width="48dp"
				android:layout_height="48dp"
				android:padding="8dp"
				android:src="@drawable/ic_expand_less_grey600_24dp"
				android:scaleType="fitCenter"
				android:tag="ic_arrow"
				app:tint="#000000" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear15"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="10dp"
			android:layout_marginBottom="10dp"
			android:orientation="vertical"
			android:tag="view_additional_info">
			<View
				android:id="@+id/view16"
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:layout_marginBottom="5dp"
				android:background="#EEEEEE"
				android:layout_marginStart="10dp"
				android:layout_marginEnd="10dp" />
			<TextView
				android:id="@+id/tv_sub_title"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="8dp"
				android:textSize="14sp"
				android:textColor="#000000"
				android:layout_marginStart="15dp"
				android:layout_marginEnd="15dp"
				android:autoLink="all"
				android:focusable="true" />
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
